{
  "name": "vite-svelte",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile dist/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile dist/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^12.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
    "sass": "^1.37.5",
    "smui-theme": "^4.2.0",
    "svelte": "^3.37.0",
    "vite": "^2.4.4"
  },
  "dependencies": {
    "@smui/button": "^4.2.0",
    "svelte-material-ui": "^4.2.0",
    "svelte-navigator": "^3.1.5"
  }
}
